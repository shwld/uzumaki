    "build:watch": "rollup -c -w"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "dotenv-cli": "^6.0.0",
    "rollup": "^2.77.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-multi-input": "^1.3.1"
  },
  "dependencies": {
    "db": "*",
    "db-pubsub": "*",
    "graphile-worker": "^0.13.0",
    "graphql-resolvers": "*"
  }
}
