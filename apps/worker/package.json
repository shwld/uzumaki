{
  "name": "worker",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "start": "dotenv node ./dist/index.js",
    "dev": "dotenv yarn tsc:watch",
    "dev2": "dotenv ts-node ./src/index.ts",
    "build": "rollup -c",
    "lint": "eslint '**' --max-warnings=0",
    "tscheck": "yarn tsc --noEmit",
    "tscheck:watch": "yarn tsc --noEmit --watch",
    "tsc:watch": "tsc-watch --noClear --onSuccess \"node ./dist/index.js\""
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "dotenv-cli": "^6.0.0",
    "rollup": "^2.77.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-multi-input": "^1.3.1",
    "tsc-watch": "^5.0.3",
    "vite": "^3.0.4"
  },
  "dependencies": {
    "db": "*",
    "db-pubsub": "*",
    "graphile-worker": "^0.13.0",
    "graphql-resolvers": "*"
  }
}
