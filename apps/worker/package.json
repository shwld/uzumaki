{
  "name": "worker",
  "version": "0.0.0",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "dotenv yarn build:watch",
    "build": "rollup -c",
    "lint": "eslint '**' --max-warnings=0",
    "tscheck": "yarn tsc --noEmit",
